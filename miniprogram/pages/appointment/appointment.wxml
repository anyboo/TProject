<!--miniprogram/pages/appointment/appointment.wxml-->
<view style="padding:2%">
    <text style="font-size:23px;font-weight:font-weight:bold;color:rgba(52,52,52,1);">中央第五街</text>
    <view class="weui-cell" style="position: relative;margin: 5px;">
      <image src="../../images/Rectangle@2x.png" style="z-index:-1;position:absolute;top:0;left:0;width:100%;height:90%"></image>
        <image class="icon" src="../../images/location@2x.png" />
        <!-- <view class="incise">|</view> -->
        <text class="location"style="position:relative;top:0;left:0;width:100%;height:90%" >{{location}}</text>
        <!-- <view class="incise">|</view> -->
        <image class="icon" src="../../images/contact@2x.png" />
      </view>
    <swiper class="home-swiper" indicator-dots="true" autoplay="{{autoplay}}" interval="{{interval}}" duration="{{duration}}">
      <swiper-item>
        <image src="https://www.teamcs.cn/images/appointment-2.png" class="slide-image" />
      </swiper-item>
      <swiper-item>
        <image src="https://www.teamcs.cn/images/appointment-3.png" class="slide-image" />
      </swiper-item>
      <swiper-item>
        <image src="https://www.teamcs.cn/images/appointment-4.png" class="slide-image" />
      </swiper-item>
      <swiper-item>
        <image src="https://www.teamcs.cn/images/appointment-5.png" class="slide-image" />
      </swiper-item>
    </swiper>
    <!-- <view class="weui-cell"> -->
      <!-- <view>
        <image class="locationinfo" src="../../images/Rectangle@2x.png" />
      </view> -->
    <!-- </view> -->
    <view class="weui-cell" bindtap="todetail" data-room="0">
      <view class="weui-cell__hd" style="position: relative;margin-right: 10px;">
        <image src="https://www.teamcs.cn/images/tearoom-1.png" style="width: 50px; height: 50px; display: block" />
        <!-- <view class="weui-badge" style="position: absolute;top: -.4em;right: -.4em;">8</view> -->
      </view>
      <view class="weui-cell__bd">
        <view>使命感</view>
        <view style="font-size: 13px;color: #343434">
          ￥68.00/小时
          <text style="margin-left:40px;">推荐1-4人</text>
        </view>
      </view>
    </view>
    <view class="weui-cell"  bindtap="todetail"  data-room="1">
      <view class="weui-cell__hd" style="position: relative;margin-right: 10px;">
        <image src="https://www.teamcs.cn/images/tearoom-2.png" style="width: 50px; height: 50px; display: block" />
        <!-- <view class="weui-badge" style="position: absolute;top: -.4em;right: -.4em;">8</view> -->
      </view>
      <view class="weui-cell__bd">
        <view>价值观</view>
        <view style="font-size: 13px;color: #343434">
          ￥68.00/小时
          <text style="margin-left:40px;">推荐1-6人</text>
        </view>
      </view>
    </view>
    <view class="weui-cell"  bindtap="todetail" data-room="2">
      <view class="weui-cell__hd" style="position: relative;margin-right: 10px;">
        <image src="https://www.teamcs.cn/images/tearoom-3.png" style="width: 50px; height: 50px; display: block" />
        <!-- <view class="weui-badge" style="position: absolute;top: -.4em;right: -.4em;">8</view> -->
      </view>
      <view class="weui-cell__bd">
        <view>责任心</view>
        <view style="font-size: 13px;color: #343434">
          ￥68.00/小时
          <text style="margin-left:40px;">推荐1-6人</text>
        </view>
      </view>
    </view>
    <view class="weui-cell"  bindtap="todetail"  data-room="3">
    <view class="weui-cell__hd" style="position: relative;margin-right: 10px;">
      <image src="https://www.teamcs.cn/images/tearoom-4.png" style="width: 50px; height: 50px; display: block" />
      <!-- <view class="weui-badge" style="position: absolute;top: -.4em;right: -.4em;">8</view> -->
    </view>
    <view class="weui-cell__bd">
      <view>凝聚力</view>
      <view style="font-size: 13px;color: #343434">
        ￥68.00/小时
        <text style="margin-left:40px;">推荐1-6人</text>
      </view>
    </view>
  </view>
</view>
