<!--miniprogram/pages/my/my.wxml-->
<image src="../../images/juxing.png" style="height:150px;width:100%"></image>
<view bindtap="toLogin">
  <view style="width:95%;height:120px;background:#fff;border-radius:10px;position:fixed;top:60px;left: 50%;transform: translate(-50%, 0);-webkit-transform: translate(-50%, 0);text-align:center;">
    <text style="color:#343434;margin-top:66px;font-size:16px;display:inline-block">{{name}}</text> 
    <text></text>
  </view>
  <view style="width:100px;height:100px;border-radius:50%;background:#fff;position:fixed;top:20px;left: 50%;transform: translate(-50%, 0);-webkit-transform: translate(-50%, 0);">
    <image src="{{imagesLogo}}" style="width:96px;height:96px;margin:2px 2px;border-radius:50%;"></image>
  </view>
</view>
<view style="margin-top:40px;padding:2%">
  <view class="weui-cell weui-cell_example " bindtap="toMyInfo">
    <view class="weui-cell__hd">
    </view>
    <view class="weui-cell__bd">
      <text class="iconfont icon-user" style="margin-right:5px"></text>
      <text style="font-size:16px;color:#343434">个人资料</text>
    </view>
    <view class="weui-cell__ft weui-cell__ft_in-access"></view>
  </view>
  <view class="weui-cell weui-cell_example " bindtap="toWallet">
    <view class="weui-cell__hd">
    </view>
    <view class="weui-cell__bd">
      <text class="iconfont icon-qianbao" style="margin-right:5px"></text>
      <text style="font-size:16px;color:#343434">钱包</text>
    </view>
    <view class="weui-cell__ft weui-cell__ft_in-access"></view>
  </view>
  <view class="weui-cell weui-cell_example ">
    <view class="weui-cell__hd">
    </view>
    <view class="weui-cell__bd" bindtap="toCard">
      <text class="iconfont icon-ticket" style="margin-right:5px"></text>
      <text style="font-size:16px;color:#343434">优惠券</text>
    </view>
    <view class="weui-cell__ft weui-cell__ft_in-access"></view>
  </view>
  <view class="weui-cell weui-cell_example " bindtap="tel">
    <view class="weui-cell__hd">
    </view>
    <view class="weui-cell__bd">
      <text class="iconfont icon-support-" style="margin-right:5px"></text>
      <text style="font-size:16px;color:#343434">联系我们</text>
    </view>
    <view class="weui-cell__ft weui-cell__ft_in-access"></view>
  </view>
</view>
<view style="width:90px;height:30px;border-radius:15px;background:#fff;font-weight:bold;position:fixed;right:5px;bottom:110px;font-weight:bold;text-align:right;padding-right:15px;line-height:30px;font-size:16px;" bindtap="toast">
  <view style="width:40px;height:40px;border-radius:50%;background:#F7B941;position:absolute;left:-5px;top:-5px;text-align:center;line-height:40px;"><text class="iconfont icon-sticker-2" style="margin:0 auto;font-size:22px;color:#fff;"></text></view>
  续单
</view>
<view style="width:90px;height:30px;border-radius:15px;background:#fff;font-weight:bold;position:fixed;right:5px;bottom:60px;font-weight:bold;text-align:right;padding-right:15px;line-height:30px"  bindtap="toast">
  <view style="width:40px;height:40px;border-radius:50%;background:#71ABF5;position:absolute;left:-5px;top:-5px;text-align:center;line-height:40px;"><text class="iconfont icon-sticker-2" style="margin:0 auto;font-size:22px;color:#fff;"></text></view>
  开门码
</view>